{
  "name": "membox-v7",
  "version": "7.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:server": "npm run dev --prefix server",
    "dev:client": "npm start --prefix client",
    "dev": "concurrently -n 'server,client' -c 'yellow,blue' \"npm run dev:server\" \"npm run dev:client\"",
    "install-client": "npm install --prefix client",
    "install-server": "npm install --prefix server",
    "start": "concurrently -n 'server,client' -c 'yellow,blue' \"npm run start:server\" node -r dotenv/config server/index.js",
    "start:server": "npm run start --prefix server",
    "start:dev": "react-app-rewired start --prefix client",
    "build": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {},
  "dependencies": {
    "concurrently": "^6.2.1"
  },
  "engines": {
    "node": "14.x"
  }
}
